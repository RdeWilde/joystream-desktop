{
  "name": "joystream-demo",
  "version": "0.0.0",
  "description": "A minimal Joystream application in electron",
  "main": "src/main.js",
  "scripts": {
    "start": "BCOIN_NO_NATIVE=1 electron-forge start",
    "test": "echo 'No test !' && exit 0",
    "package": "BCOIN_NO_NATIVE=1 electron-forge package",
    "make": "BCOIN_NO_NATIVE=1 electron-forge make"
  },
  "keywords": [
    "Joystream",
    "demo"
  ],
  "author": "Me",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "cmake-js": "^3.4.1",
    "electron-prebuilt-compile": "1.6.4",
    "mobx-react-devtools": "^4.2.11",
    "node-gyp": "^3.6.0",
    "webpack": "^2.3.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bcoin": "1.0.0-beta.12",
    "bootstrap": "^4.0.0-alpha.6",
    "debug": "^2.6.0",
    "joystream-node": "git+ssh://git@github.com/JoyStream/joystream-node.git#development",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "mobx": "^3.1.7",
    "mobx-react": "^4.1.3",
    "qrcode-react": "^0.1.12",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "tether": "^1.4.0"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "name": "Joystream"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "JoyStream",
        "name": "joystream-electron"
      },
      "windowsStoreConfig": {
        "packageName": "Joystream"
      }
    }
  }
}
